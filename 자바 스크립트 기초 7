    <script>

        let dan = prompt("몇단 출력?");

        for (let i = 0; i <= 9; i++) {
            // for (let i = 0; i <=dan; i++)
            document.write(`${dan} * ${i} = ${dan * i} <br>`)

        }

    </script>

